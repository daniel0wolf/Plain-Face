  <section>
    <defs>
      <symbol id="propellersRap">
        <g transform="scale(0)">
          <defs>
            <symbol id="propellers" x="50%" y="58%">
              <g pointer-events="visible" transform="translate(0%,0%)">
                <animateTransform attributeType="rotate" from="-80" to="80" final="restore" begin="load;enable" end="disable" dur="0.3" repeatCount="indefinite" easing="linear" />
                <animateTransform attributeType="translate" from="0,-3" to="0,3" begin="load;enable" end="disable" final="keep" dur="2.5" repeatCount="indefinite" easing="ease" />
                <image href="images/propellers.png" class="propellers" />
              </g>
            </symbol>
          </defs>
          <svg width="100%" height="100%">
            <use id="propellersAnimation" href="#propellers" width="100%" height="100%" />
          </svg>
          <animateTransform attributeType="scale" from="0" to="1" begin="load;enable" end="disable" final="keep" dur="5" repeatCount="1" easing="ease-out" />
          <animateTransform attributeType="translate" from="150,250" to="0,0" begin="load;enable" end="disable" final="keep" dur="5" repeatCount="1" easing="ease-out" />
          <animate attributeName="opacity" begin="load;enable" end="disable" from="0" to="1" dur="1.3" final="keep" repeatCount="1" easing="ease-in" />
          <animateTransform attributeType="rotate" from="0" to="360" begin="load;enable" end="disable" dur="2" final="keep" repeatCount="1" easing="ease-out" />
        </g>
      </symbol>
    </defs>
    <svg width="100%" height="100%">
      <use id="propellersMove" href="#propellersRap" width="100%" height="100%" />
    </svg>
  </section>
  <!-- //plane's propellers -->
  <text id="date" />
  <section>
    <defs>
