          <animateTransform attributeType="translate" from="50%,190" to="50%,228" begin="load;enable" end="disable" final="keep" dur="1.5" repeatCount="indefinite" easing="ease-in" />
          <animate attributeName="opacity" begin="load;enable" end="disable" from="1" to="0.2" dur="1.5" final="keep" repeatCount="indefinite" easing="ease-in" />
          <image href="images/cloudsMove2.png" class="clouds" />
        </g>
      </symbol>
    </defs>
    <svg width="100%" height="100%">
      <use id="cloudsAnimation1" href="#clouds1" width="100%" height="100%" />
    </svg>
  </section>
